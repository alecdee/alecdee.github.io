	#[quot]<<=1
	.tmp  .tmp  ?+1
	.tmp  .quot ?+1
	.quot .tmp  ?+1
	#[rem]<<=1
	.tmp  .tmp  ?+1
	.tmp  .rem  ?+1
	.rem  .tmp  .loop
.ret:
	#Set quotient.
	.quot .r0:0 ?+1
	.tmp  .tmp  ?+1
	.tmp  .quot ?+1
	.r1:0 .tmp  ?+1
	#Set remainder.
	.rem  .r2:0 ?+1
	.tmp  .tmp  ?+1
	.tmp  .rem  ?+1
	.r3:0 .tmp  ?+1
	0     0     .a6:6
.divz:
	#Abort.
	0-1 0xd0 0
