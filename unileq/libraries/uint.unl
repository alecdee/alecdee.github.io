	.r1   .r1   ?+1
	.r1   .tmp  ?+1
	#Get remainder.
	.tmp  .tmp  ?+1
	.tmp  .a3:3 ?+1
	.r2   .r2   ?+1
	.r2   .tmp  ?+1
	.r3   .r3   ?+1
	.r3   .tmp  ?+1
	#Get numerator.
	.tmp  .tmp  ?+1
	.tmp  .a4:4 ?+1
	.p0   .p0   ?+1
	.p0   .tmp  ?+1
	.tmp  .tmp  ?+1
	.tmp  .p0:0 ?+1
	.num  .num  ?+1
	.num  .tmp  ?+1
	#Get denominator.
	.tmp  .tmp  ?+1
	.tmp  .a5:5 ?+1
	.p1   .p1   ?+1
	.p1   .tmp  ?+1
	.nden .nden ?+1
	.nden .p1:0 ?+1
	#If [den]=0, abort
	.nden .z    .divz
	.nden .z-1  ?+1
	.den  .den  ?+1
	.den  .nden ?+1
	#Start loop.
	.quot .quot ?+1
	.rem  .rem  ?+1
	.i    .bits ?+1
.loop:
	#[rem]+=[num]>>63, [num]<<=1
	.tmp  .tmp  ?+1
	.tmp  .num  ?+1
	.tmp  .z+1  ?+1
	.num  .tmp  ?+4
	.rem  .z-1  ?+1
	#if [rem]>=[den], [quot]+=1
	.rem  .den  ?+7
	.rem  .z+1  ?+1
	.quot .z-1  ?+4
	.rem  .nden ?+1
